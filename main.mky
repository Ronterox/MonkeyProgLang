let map = fn(arr, f) {
	let iter = fn(arr, acc) {
		if (len(arr) == 0) {
			return acc
		}
		iter(rest(arr), push(acc, f(first(arr))))
	}
	iter(arr, [])
}

let reduce = fn(arr, acc, f) {
	let iter = fn(arr, acc) {
		if (len(arr) == 0) {
			return acc
		}
		iter(rest(arr), f(first(arr), acc))
	}
	iter(arr, acc)
}

let sum = fn(arr) { reduce(arr, 0, fn(n, acc) { n + acc }) };

let a = [1, 2, 3, 4];
sum(map(a, fn(x) { x * 100 }));
