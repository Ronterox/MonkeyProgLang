eval(read("grammar.mky"))

let say = "sayfunc"; let bye = "byefunc";
let newPerson = fn(name, age) {{
	"name": name,
	"age": age,
	say: fn(me) { "Hi my name is " + me["name"] + " and I'm " + string(me["age"]) },
	bye: fn(me) { "Bye from " + me["name"] },
}}

let people = [
	newPerson("Alice", 25),
	newPerson("Luis Monte", 50)
];

for(0, 10, echo)
apply(people, say)

