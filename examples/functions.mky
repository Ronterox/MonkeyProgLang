let for = fn(from, to, f) {
	let val = f(from)
	// TODO: I need greater eq
	if from > to { return val; }
	return for(from + 1, to, f)
}

let map = fn(arr, f) {
	let iter = fn(arr, acc) {
		if len(arr) == 0 {
			return acc
		}
		iter(tail(arr), push(acc, f(head(arr))))
	}
	iter(arr, [])
}

let foreach = map;

let reduce = fn(arr, acc, f) {
	let iter = fn(arr, acc) {
		if len(arr) == 0 {
			return acc
		}
		iter(tail(arr), f(head(arr), acc))
	}
	iter(arr, acc)
}
